<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Nature's Pretty Sweet</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="TKKN_Desert" or defName="TKKN_Savanna" or defName="TKKN_Oasis"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="TKKN_RedwoodForest" or defName="TKKN_Grasslands"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Woodland</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="TKKN_VolcanicFlow"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Jungle</CamoType>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>