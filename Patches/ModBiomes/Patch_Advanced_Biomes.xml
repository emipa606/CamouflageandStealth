<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Advanced Biomes</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="PoisonForest"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Jungle</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="Savanna"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="Volcano"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Stone</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="Wasteland"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="Wetland"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Woodland</CamoType>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>