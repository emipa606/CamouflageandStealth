<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>SimplyMoreFloors</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/TerrainDef[@Name="SMF_FloorBaseCarpet"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Urban</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/TerrainDef[@Name="SMF_FloorMetalBase"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Urban</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/TerrainDef[@Name="SMF_FloorLuxoriousBase"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Stone</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/TerrainDef[@Name="SMF_FloorBaseWood"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Woodland</CamoType>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>