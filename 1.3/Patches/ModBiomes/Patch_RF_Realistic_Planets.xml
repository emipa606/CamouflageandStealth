<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Realistic Planets - Fan Update</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_Grassland"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Woodland</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_Savanna"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_Steppes"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Woodland</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_TemperateDesert"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_Permafrost"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Arctic</CamoType>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>/Defs/BiomeDef[defName="RRP_Oasis"]</xpath>
          <value>
            <li Class="CompCamo.CompCamoDefs">
              <CamoType>Desert</CamoType>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>